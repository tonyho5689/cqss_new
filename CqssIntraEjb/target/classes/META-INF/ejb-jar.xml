<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE ejb-jar PUBLIC "-//Sun Microsystems, Inc.//DTD Enterprise JavaBeans 2.0//EN" "http://java.sun.com/dtd/ejb-jar_2_0.dtd">
<ejb-jar>
  <display-name>CqssIntraEjb</display-name>
  <enterprise-beans>
    <!-- Session Beans -->
    <session>
      <ejb-name>CqssIntraEjb</ejb-name>
      <home>com.ocbc.hk.apps.cqss.ejb.CqssIntraEjbHome</home>
      <remote>com.ocbc.hk.apps.cqss.ejb.CqssIntraEjb</remote>
      <ejb-class>com.ocbc.hk.apps.cqss.ejb.CqssIntraEjbBean</ejb-class>
      <session-type>Stateless</session-type>
      <transaction-type>Container</transaction-type>
    </session>
  </enterprise-beans>
  <assembly-descriptor>
    <container-transaction>
      <method>
        <ejb-name>CqssIntraEjb</ejb-name>
        <method-name>*</method-name>
      </method>
      <trans-attribute>Required</trans-attribute>
    </container-transaction>
  </assembly-descriptor>
</ejb-jar>
